def zadacha_4(stepin,chislo):
    x_nachalnoe=1
    x_new=0
    tochost=0.0001
    print("Начало работы")
    while(1):
        print(x_new)
        x_new=1/stepin*((stepin-1)*x_nachalnoe+chislo/(x_nachalnoe**(stepin-1)))
        if(abs(x_new-x_nachalnoe)<tochost):
            break
        x_nachalnoe=x_new

print("Введите степень корня и подкоренное число")
zadacha_4(int (input()),int (input()))
